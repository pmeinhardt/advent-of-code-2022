#!/usr/bin/env python3

import sys

def lt(a, b):
    if type(a) == type(b) == int:
        return a < b

    if type(a) == type(b) == list:
        for x, y in zip(a, b):
            if x == y:
                continue
            return lt(x, y)
        return len(a) < len(b)

    if type(a) == list:
        return lt(a, [b])

    if type(b) == list:
        return lt([a], b)

    raise "unknown types"

if __name__ == '__main__':
    results = []

    while True:
        try:
            left, right = map(eval, [next(sys.stdin), next(sys.stdin)])
            results.append(lt(left, right))
            next(sys.stdin)
        except Exception as e:
            break

    print(sum([i for i, x in enumerate(results, start=1) if x]))
